{
    "env": {
        "browser": true,
        "commonjs": false,
        "es6": true,
        "node": true,
        "jquery": true,
        "jest":true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true,
            "js": true
        },
        "sourceType": "module",
        "ecmaVersion": 2017
    },
    "rules": {
        "no-const-assign": "warn",
        "no-this-before-super": "warn",
        "no-undef": "warn",
        "no-unreachable": "warn",
        "no-unused-vars": "off",
        "constructor-super": "warn",
        "valid-typeof": "warn"
    },
    "globals": {
        "IMWS": false,
        "IMatch": false,
        "IMatchTranslate": false,
        "ol":false
    }
}